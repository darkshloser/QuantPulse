# =============================================================================
# QuantPulse - Environment variables template
# Copy to .env and fill in your values. Never commit .env to version control.
# =============================================================================

# PostgreSQL
POSTGRES_USER=
POSTGRES_PASSWORD=
POSTGRES_DB=

# Database / Redis URLs (used by backend services)
DATABASE_URL=postgresql://user:password@localhost:5432/quantpulse
REDIS_URL=redis://localhost:6379/0

# Admin seed account (created automatically on first startup)
ADMIN_USERNAME=
ADMIN_EMAIL=
ADMIN_PASSWORD=

# JWT (generate with: python -c "import secrets; print(secrets.token_hex(32))")
JWT_SECRET_KEY=

# SEC EDGAR User-Agent (required format: "AppName contact@email")
SEC_USER_AGENT=

# Slack notifications
SLACK_WEBHOOK_URL=
SLACK_ENABLED=false

# Services (used by frontend / inter-service calls)
SYMBOL_SERVICE_URL=http://localhost:8001
MDR_SERVICE_URL=http://localhost:8002
DAS_SERVICE_URL=http://localhost:8003
NOTIFIER_SERVICE_URL=http://localhost:8004

# Scheduler
ANALYZER_SCHEDULE_TIME=02:00

# Environment
DEBUG=false
LOG_LEVEL=INFO
